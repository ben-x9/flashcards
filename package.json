{
  "name": "flashcards",
  "version": "0.0.1",
  "description": "a flashcard app for language learning",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --content-base app --history-api-fallback --progress --inline --hot --host 0.0.0.0 --debug --devtool cheap-module-eval-source-map --output-pathinfo",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --optimize-minimize && mkdir -p build/ && mv bundle.js build/ && cp app/index.html build/"
  },
  "author": "Ben Lambert",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ben-x9/flashcards.git"
  },
  "license": "MIT",
  "devDependencies": {
    "ts-loader": "^1.3.3",
    "typescript": "^2.1.4",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "csstips": "0.0.3",
    "snabbdom": "github:snabbdom/snabbdom",
    "typestyle": "^0.22.1"
  }
}
